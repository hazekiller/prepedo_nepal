{
  "expo": {
    "name": "Prepedo Nepal",
    "slug": "prepedo-nepal",
    "version": "1.0.0",
    "scheme": "prepedo",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#000000",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "url": "https://u.expo.dev/b1234567-89ab-4cde-9012-3456789fabcd",
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.prepedo.nepal",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to your camera for profile and document uploads."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.prepedo.nepal",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "expo-router"
        }
      ],
      "expo-font",
      "expo-splash-screen",
      "expo-secure-store"
    ],
    "extra": {
      "router": {
        "origin": "expo-router"
      },
      "eas": {
        "projectId": "b1234567-89ab-4cde-9012-3456789fabcd"
      },
      "API_BASE_URL": "http://192.168.1.68:7249",
      "APP_ENV": "development",
      "ENABLE_DEBUG": true
    },
    "owner": "hazekiller"
  }
}
